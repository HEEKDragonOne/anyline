<?xml version="1.0" encoding="UTF-8"?>
<configs>
	<config key="default">
		<!-- 第一页 -->
		<property key="STYLE_BUTTON_FIRST">First</property>
		<!-- 上一页 -->
		<property key="STYLE_BUTTON_PREV"></property>
		<!-- 下一页 -->
		<property key="STYLE_BUTTON_NEXT"></property>
		<!-- 最后页 -->
		<property key="STYLE_BUTTON_LAST">Last</property>
		<!-- 数据统计格式 -->
		<property key="STYLE_STAT_FORMAT"></property>
		<!-- 跳转到格式 -->
		<property key="STYLE_JUMP_FORMAT"></property>

		<!-- 下标数量 -->
		<property key="VAR_PAGE_RANGE">5</property>
		<!-- 显示上一页下一页 -->
		<property key="VAR_SHOW_BUTTON">true</property>
		<!-- 显示下标 -->
		<property key="VAR_SHOW_INDEX">true</property>
		<!-- 显示数据统计 -->
		<property key="VAR_SHOW_STAT">true</property>
		<!-- 跳转到 -->
		<property key="VAR_SHOW_JUMP">false</property>
		
		
		<!-- 每页默认显示多少条 -->
		<property key="VAR_PAGE_DEFAULT_VOL">10</property>
		<!-- 用户端是否可以设置每页多少条 -->
		<property key="VAR_CLIENT_SET_VOL_ENABLE">true</property>
		<!-- 用户端指定每页多少条的key -->
		<property key="KEY_PAGE_ROWS">_anyline_page_rows</property>
		<!-- 用户端指定当前第几页的key -->
		<property key="KEY_PAGE_NO">_anyline_page</property>
		<!-- 加载更多格式 -->
		<property key="STYLE_LOAD_MORE_FORMAT"></property>
		<property key="STYLE_FILE_PATH">//deepbit.oss-cn-shanghai.aliyuncs.com/plugin/navi/skin.css</property>
		<property key="SCRIPT_FILE_PATH">//deepbit.oss-cn-shanghai.aliyuncs.com/plugin/navi/navi.js</property>
	</config>
</configs>